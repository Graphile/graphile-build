{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "array-type": [true, "generic"],
    "interface-name": [true, "never-prefix"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
